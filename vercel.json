{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/$1.html"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "status": 404,
      "dest": "/404.html"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "outputDirectory": "public",
  "framework": "static"
}
